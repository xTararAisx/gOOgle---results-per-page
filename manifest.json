{
  "name": "gOOgle results per page",
  "version": "1.0",
  "manifest_version": 3,
   "author": "TarArAis",
    "description": "Customize your Google search experience by choosing how many results are displayed per page. Bring back control to suit your needs.",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "icons": {
    "16": "icons/ico16.png",
	"32": "icons/ico32.png",
	"48": "icons/ico48.png",
	"128": "icons/ico128.png"	
  },
  "host_permissions": [
    "*://www.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "declarative_net_request": {
    "rule_resources": []
  }
}
